@model IEnumerable<mutfakdapper.Models.TatliModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Ekle", "EY", null, new { @class = "btn btn-warning" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TatliAdi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SosTercihi)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TatliAdi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SosTercihi)
        </td>
        <td>
            @Html.ActionLink("Edit", "EY", new {  id=item.TatliNo }, new { @class = "btn btn-warning" })
            @Html.ActionLink("Delete", "Delete", new { id=item.TatliNo }, new { @class = "btn btn-warning" })
        </td>
    </tr>
}

</table>
