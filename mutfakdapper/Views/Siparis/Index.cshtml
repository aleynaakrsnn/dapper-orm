@model IEnumerable<mutfakdapper.Models.SiparisModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Ekle", "EY", null, new { @class = "btn btn-warning" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SiparisAdi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SiparisAdresi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SiparisTelefon)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.SiparisAdi)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.SiparisAdresi)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.SiparisTelefon)
    </td>
    <td>
        @Html.ActionLink("Edit", "EY", new { id=item.SiparisNo }, new { @class = "btn btn-warning" }) |
        @Html.ActionLink("Delete", "Delete", new { id=item.SiparisNo }, new { @class = "btn btn-warning" })
    </td>
</tr>
}

</table>
